_G.cum = true

-- Ağırlığı kuşan
game:GetService("ReplicatedStorage").RemoteEvent:FireServer({{"Weight", "Weight4"}})

while _G.cum do
    for i = 1, 5 do  -- Aynı anda 5 kez göndererek hız artırıldı
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer({{"+MS24"}})
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer({{"+JF24"}})
    end
    task.wait() -- Minimum bekleme (bunu kaldırarak maksimum hız alınabilir)
end

-- Ağırlığı çıkar
game:GetService("ReplicatedStorage").RemoteEvent:FireServer({{"Weight", "Unequip"}})
